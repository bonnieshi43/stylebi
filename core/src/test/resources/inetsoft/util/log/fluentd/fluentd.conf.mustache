<source>
  @type forward
  port {{port}}
  {{#useTls}}
  <transport tls>
    min_version TLS1_2
    max_version TLS1_3
    insecure false
    ca_path {{caCrtFile}}
    cert_path {{domainCrtFile}}
    private_key_path {{domainKeyFile}}
    private_key_passphrase {{domainKeyPassword}}
  </transport>
  {{/useTls}}
  {{#useAuthentication}}
  <security>
    self_hostname localhost
    shared_key {{sharedKey}}
    allow_anonymous_source true
    {{#useAuthorization}}
    user_auth true
    <user>
      username {{username}}
      password {{password}}
    </user>
    {{/useAuthorization}}
    {{^useAuthorization}}
    user_auth false
    {{/useAuthorization}}
  </security>
  {{/useAuthentication}}
</source>
<match **>
  @type stdout
</match>
